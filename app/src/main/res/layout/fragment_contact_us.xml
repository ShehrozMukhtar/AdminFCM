<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".main_activity.contact_us.ContactUsFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FF2E8828"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:id="@+id/backward"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:src="@drawable/arrow_back"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="30dp"
            />
        <TextView
            android:id="@+id/fcm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/irish"
            android:text="CONTACT US"
            android:textColor="@color/white"
            android:textSize="44dp"
            android:textStyle="bold"
           app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/balance"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/fcm"
            android:layout_marginTop="20dp"
            >

            <androidx.cardview.widget.CardView
                android:id="@+id/ahmad"
                android:layout_width="match_parent"
                android:layout_height="116dp"
                android:layout_marginTop="70dp"
                app:cardCornerRadius="8dp"
                app:cardElevation="4dp"
                android:layout_marginHorizontal="20dp"
                app:cardUseCompatPadding="true"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="90dp"
                    android:layout_height="70dp"
                    android:layout_marginTop="15dp"
                    android:src="@drawable/ahmad"
                    />
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="AHMAD"
                    android:textColor="@color/black"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:gravity="top"
                    android:textSize="17dp" />
                <TextView
                    android:id="@+id/p_ahmad"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="+92 312 7861329"
                    android:textColor="#0000FF"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="35dp"
                    android:gravity="top"
                    android:textSize="17dp" />
                <TextView
                    android:id="@+id/a_mail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="ahmadchistie6458@gamil.com"
                    android:textColor="#0000FF"
                    android:layout_marginLeft="34dp"
                    android:layout_marginTop="60dp"
                    android:gravity="top"
                    android:textSize="15dp" />
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/haris"
                android:layout_width="match_parent"
                android:layout_height="116dp"
                android:layout_marginTop="30dp"
                app:cardCornerRadius="8dp"
                app:cardElevation="4dp"
                android:layout_marginHorizontal="20dp"
                app:cardUseCompatPadding="true"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/ahmad">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="90dp"
                    android:layout_height="70dp"
                    android:layout_marginTop="15dp"
                    android:src="@drawable/haris"
                    />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="HARIS"
                    android:textColor="@color/black"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:gravity="top"
                    android:textSize="17dp" />
                <TextView
                    android:id="@+id/p_haris"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="+92 303 6000517"
                    android:textColor="#0000FF"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="35dp"
                    android:gravity="top"
                    android:textSize="17dp" />
                <TextView
                    android:id="@+id/h_mail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="haaris5421@gamil.com"
                    android:textColor="#0000FF"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="60dp"
                    android:gravity="top"
                    android:textSize="17dp" />
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/shehroz"
                android:layout_width="match_parent"
                android:layout_height="116dp"
                android:layout_marginTop="30dp"
                app:cardCornerRadius="8dp"
                app:cardElevation="4dp"
                android:layout_marginHorizontal="20dp"
                app:cardUseCompatPadding="true"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/haris">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="90dp"
                    android:layout_height="70dp"
                    android:layout_marginTop="15dp"
                    android:src="@drawable/sheri"
                    />

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="SHEHROZ"
                    android:textColor="@color/black"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="top"
                    android:textSize="17dp" />
                <TextView
                    android:id="@+id/p_shehroz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="+92 343 4139070"
                    android:textColor="#0000FF"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="35dp"
                    android:gravity="top"
                    android:textSize="17dp" />
                <TextView
                    android:id="@+id/m_mail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="@font/robotoregular"
                    android:text="shehroz416rgc@gamil.com"
                    android:textColor="#0000FF"
                    android:layout_marginLeft="35dp"
                    android:layout_marginTop="60dp"
                    android:gravity="top"
                    android:textSize="17dp" />
            </androidx.cardview.widget.CardView>


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>