<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_nav_graph"
    app:startDestination="@id/dashBoardFragment">

    <fragment
        android:id="@+id/dashBoardFragment"
        android:name="com.example.flourcrisismanagment.main_activity.dashboard_fragment.DashBoardFragment"
        android:label="fragment_dash_board"
        tools:layout="@layout/fragment_dash_board" >
        <action
            android:id="@+id/action_dashBoardFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_dailyHistoryFragment"
            app:destination="@id/dailyHistoryFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_stockFragment"
            app:destination="@id/stockFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_feedBackFragment"
            app:destination="@id/feedBackFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_aboutUsFragment"
            app:destination="@id/aboutUsFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_contactUsFragment"
            app:destination="@id/contactUsFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_termConditionFragment"
            app:destination="@id/termConditionFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.flourcrisismanagment.main_activity.profile_fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/dailyHistoryFragment"
        android:name="com.example.flourcrisismanagment.main_activity.daily_history.DailyHistoryFragment"
        android:label="fragment_daily_history"
        tools:layout="@layout/fragment_daily_history" />
    <fragment
        android:id="@+id/stockFragment"
        android:name="com.example.flourcrisismanagment.main_activity.stock_fragment.StockFragment"
        android:label="fragment_stock"
        tools:layout="@layout/fragment_stock" >
        <action
            android:id="@+id/action_stockFragment_to_requestFragment"
            app:destination="@id/requestFragment" />
    </fragment>
    <fragment
        android:id="@+id/requestFragment"
        android:name="com.example.flourcrisismanagment.main_activity.request_fragment.RequestFragment"
        android:label="fragment_request"
        tools:layout="@layout/fragment_request" />
    <fragment
        android:id="@+id/feedBackFragment"
        android:name="com.example.flourcrisismanagment.main_activity.feed_back.FeedBackFragment"
        android:label="fragment_feed_back"
        tools:layout="@layout/fragment_feed_back" />
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="com.example.flourcrisismanagment.AboutUsFragment"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" />
    <fragment
        android:id="@+id/contactUsFragment"
        android:name="com.example.flourcrisismanagment.main_activity.contact_us.ContactUsFragment"
        android:label="fragment_contact_us"
        tools:layout="@layout/fragment_contact_us" />
    <fragment
        android:id="@+id/changeEmailFragment"
        android:name="com.example.flourcrisismanagment.main_activity.email_change.ChangeEmailFragment"
        android:label="fragment_change_email"
        tools:layout="@layout/fragment_change_email" />
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.example.flourcrisismanagment.main_activity.privacy_policy.PrivacyPolicyFragment"
        android:label="fragment_privacy_policy"
        tools:layout="@layout/fragment_privacy_policy" />
    <fragment
        android:id="@+id/termConditionFragment"
        android:name="com.example.flourcrisismanagment.main_activity.term_condition.TermConditionFragment"
        android:label="fragment_term_condition"
        tools:layout="@layout/fragment_term_condition" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.flourcrisismanagment.main_activity.settings.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_changeEmailFragment"
            app:destination="@id/changeEmailFragment" />
    </fragment>
</navigation>